    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^16.14.2",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.7",
    "axios": "^0.21.1",
    "classnames": "^2.2.6",
    "history": "^5.0.0",
    "http-proxy-middleware": "^1.0.6",
    "lodash.groupby": "^4.6.0",
    "lodash.keys": "^4.2.0",
    "lodash.uniqby": "^4.7.0",
    "node-sass": "^4.0.0",
    "primeflex": "^2.0.0",
    "primeicons": "^4.1.0",
    "primereact": "^6.0.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-icons": "^4.2.0",
    "react-loader-spinner": "^4.0.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "react-transition-group": "^4.4.1",
