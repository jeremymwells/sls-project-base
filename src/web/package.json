    "styled-components": "^5.2.1",
    "typescript": "^4.0.3",
    "web-vitals": "^0.2.4"
  },
  "scripts": {
    "start": "FAST_REFRESH=false react-scripts start",
    "build:local": "REACT_APP_STAGE=local PUBLIC_URL=/local react-scripts build",
    "build:dev": "REACT_APP_STAGE=dev PUBLIC_URL=/dev react-scripts build",
    "build:prd": "REACT_APP_STAGE=prd react-scripts build",
    "dev:local": "REACT_APP_STAGE=local PUBLIC_URL=/local BROWSER=none yarn start",
    "dev:dev": "REACT_APP_STAGE=dev PUBLIC_URL=/dev BROWSER=none yarn start",
    "dev:prod": "REACT_APP_STAGE=prod PUBLIC_URL=/prod BROWSER=none yarn start",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "react-scripts": "react-scripts",
    "buildAndExport:target_env": "npm run \"build:$TARGET_ENV\"",
    "buildAndExport:prd": "NODE_ENV=production TARGET_ENV=prd npm run buildAndExport:target_env",
    "buildAndExport:dev": "NODE_ENV=production TARGET_ENV=dev npm run buildAndExport:target_env",
    "buildAndExport:local": "NODE_ENV=production TARGET_ENV=local npm run buildAndExport:target_env"
  },
  "eslintConfig": {
    "ignorePatterns": [
      "*.tsx",
      "*.ts"
    ],
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
