{
  "name": "project-base",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "proxy": "./setupProxy.js",
  "scripts": {
    "android": "expo-cli start --android",
    "build": "node ../../runner.js build",
    "eject": "expo-cli eject",
    "ios": "expo-cli start --ios",
    "lint": "eslint ./**/*.tsx  --resolve-plugins-relative-to='.'",
    "lint:ci": "npm run lint",
    "lint:fix": "eslint ./**/*.tsx  --resolve-plugins-relative-to='.' --fix",
    "localProxy": "concurrently --kill-others 'node localProxy.js' 'expo-cli web --localhost --non-interactive'",
    "start": "node ../../runner.js run --target=web -l --publicUrl='http://localhost:3000'",
    "test-unit": "node ../../runner.js run --target=unit --command='jest --config=./jest.config.ts --coverage --watchAll'",
    "test-unit:ci": "CI=true node ../../runner.js run --target=unit --command='jest --config=./jest.config.ts --coverage'",
    "test-integration": "concurrently \"cd ../ && npm start && cd -\" \"node ../../runner.js run --target='cypress' -l --publicUrl='http://localhost:3000'\"",
    "test-integration:ci": "CI=true concurrently  -k -s first \"cd ../ && npm start && cd -\" \"node ../../runner.js run --target='cypress' --publicUrl='http://localhost:3000'\"",
    "test": "jest --watchAll"
  },
  "dependencies": {
    "@expo/vector-icons": "^13.0.0",
    "@react-navigation/bottom-tabs": "^6.0.5",
    "@react-navigation/native": "^6.0.2",
    "@react-navigation/native-stack": "^6.1.0",
    "axios": "^0.27.2",
    "expo": "~45.0.0",
    "expo-asset": "~8.5.0",
    "expo-constants": "~13.1.1",
    "expo-font": "~10.1.0",
    "expo-linking": "~3.1.0",
    "expo-splash-screen": "~0.15.1",
    "expo-status-bar": "~1.3.0",
    "expo-system-ui": "~1.2.0",
    "expo-web-browser": "~10.2.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-native": "0.68.2",
    "react-native-safe-area-context": "4.2.4",
    "react-native-screens": "~3.11.1",
    "react-native-web": "0.17.7"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@expo/webpack-config": "^0.16.24",
    "@types/react": "~17.0.21",
    "@types/react-native": "~0.66.13",
    "cypress": "^9.7.0",
    "eslint": "^8.16.0",
    "expo-cli": "^5.4.9",
    "http-proxy-middleware": "^2.0.6",
    "jest": "^26.6.3",
    "jest-expo": "~45.0.0",
    "react-test-renderer": "17.0.2",
    "typescript": "~4.3.5",
    "webpack-dev-server": "~3.11.0"
  },
  "private": true
}
