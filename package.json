{
  "name": "projectbase",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "deploy:dev": "node deploy.js dev",
    "deploy:nonprod": "node deploy.js",
    "deploy:prod": "node deploy.js prod",
    "lint": "./node_modules/.bin/eslint --ext .js,.ts src",
    "lint:fix": "npm run lint -- --fix",
    "local:api": "node ./run.js api",
    "local:web": "cd src/web && npm start && cd -",
    "preinstall": "cd src/web && (npm install) && cd -",
    "prelint": "cd src/web && (npm run lint) && cd -",
    "prelint:fix": "cd src/web && (npm run lint:fix) && cd -",
    "start": "concurrently --kill-others \"npm run local:api\" \"npm run local:web\"",
    "test": "NODE_ENV=test mocha -r ts-node/register src/{,!(node_modules|web)/**/}/*.spec.js --reporter markdown"
  },
  "author": "",
  "license": "ISC",
