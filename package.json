    "eslint-plugin-import": "^2.22.1",
    "localtunnel": "^2.0.0",
    "mocha": "^8.1.1",
    "node-debug": "^0.1.0",
    "nodemon": "^2.0.6",
    "serverless": "^3.10.2",
