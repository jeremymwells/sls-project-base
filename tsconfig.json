{
    "compilerOptions": {
      "lib": ["es2017"],
      "removeComments": true,
      "moduleResolution": "node",
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "sourceMap": true,
      "target": "es2017",
      "outDir": ".tsout",
      "experimentalDecorators": true
    },
    "include": ["./**/*.ts" ],
    "exclude": [
      "node_modules/**/*",
      ".serverless/**/*",
      ".webpack/**/*",
      "_warmup/**/*",
      ".vscode/**/*"
    ]
  }
  